<UserControl x:Class="Sklop1.UserControls.Iskanje"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:Sklop1.UserControls"
             mc:Ignorable="d"
             d:DesignHeight="40" d:DesignWidth="550">
    <Grid>
        <StackPanel Orientation="Horizontal">
            <TextBox TextChanged="OnTextChange" x:Name="iskanjeText" Margin="10 10 0 10" Width="500">Search</TextBox>
            <Image Margin="10 10 10 10" Height="20" Width="20" Source="/search.png" />
        </StackPanel>
    </Grid>
</UserControl>
